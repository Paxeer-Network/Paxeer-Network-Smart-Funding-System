  "name": "@paxeer/paxeer-connect",
  "version": "1.0.0",
  "description": "Paxeer Connect SDK â€” Unified SmartWallet integration for protocol developers",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    },
    "./vue": {
